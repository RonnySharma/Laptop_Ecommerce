<div class="row">
    <div class="col-10">
        <h2 class="text-primary">
            <i class="fas fa-list"></i>Users List
        </h2>
    </div>
    <div class="col-2">
        <a asp-area="Identity" asp-page="/Account/Register" class="btn btn-success">
            <i class="fas fa-plus"></i>&ensp;Create User
        </a>
    </div>
</div>
<div class="border p-2 m-2">
    <table id="tblData" class="table table-bordered table-striped table-hover table-responsive active">
        <thead>
            <tr>
                <th class="text-center text-primary">Name</th>
                <th class="text-center text-primary">Email</th>
                <th class="text-center text-primary">Phone Number</th>
                <th class="text-center text-primary">Company</th>
                <th class="text-center text-primary">Role</th>
                <th class="text-center text-primary"></th>
            </tr>
        </thead>
    </table>
</div>
@section scripts{
    <script src="~/js/user.js"></script>
}