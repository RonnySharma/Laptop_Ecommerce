<div class="row">
    <div class="col-10">
        <h2 class="text-primary">
        <i class="fas fa-list-alt"></i>Laptops List
        </h2>
    </div>
    <div class="col-2">
        <a asp-action="Upsert"class="btn btn-success">
            <i class="fas fa-plus"></i>&nbsp;Add Laptop
        </a>
    </div>
</div>
<div class="border container p-2 m-2">
    <table id="tblData"class="table table-striped table-bordered table-hover table-responsive active">
        <thead>
            <tr>
                <th class="text-center text-primary">Brand</th>
                <th class="text-center text-primary">Model Name</th>
                <th class="text-center text-primary">Processor</th>
                <th class="text-center text-primary">Graphics Card</th>
                <th class="text-center text-primary">Ram Memory</th>
                <th class="text-center text-primary">Storage Space</th>
                <th class="text-center text-primary">Price</th>
                <th class="text-center text-primary">Actions</th>
            </tr>
        </thead>
    </table>
</div>
@section scripts
{
    <script src="~/js/laptop.js"></script>
}